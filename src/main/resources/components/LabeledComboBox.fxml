<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<fx:root type="HBox" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml" spacing="10.0" alignment="CENTER_LEFT">
    <padding>
        <Insets top="15.0" right="5.0" />
    </padding>
    <Label fx:id="label" layoutX="15.0" layoutY="10.0" styleClass="bold-label" text="Text"
           textFill="#000000ea">
        <font>
            <Font size="12.0"/>
        </font>
    </Label>
    <ComboBox fx:id="comboBox" layoutX="15.0" layoutY="33.0"
              HBox.hgrow="ALWAYS">
    </ComboBox>
</fx:root>